FizzBuzz

int i;

for i=1, i<=100, i++ {
	
	if i = %3 && i = %5 {
		print = "fizzbuzz";
	
	} else {
	
		if i = %3{
			print = "fizz";

		} else {
		
			if i = %5{
				print = "buzz";
			} else
				print = i;
		}
	}

}
	

	

Tablas

int i;
int j;
int res;

for i=1, i<=10, i++;{

	for j=1, j<=10, j++;{
		
		res = i*j;
		print = res;
	}
}

